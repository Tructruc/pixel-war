generator client {
   provider = "prisma-client-js"
}

model User {
   id              String    @id @default(uuid())
   nextPlacementAt DateTime  @default(now())
}

model Canva {
   x         Int
   y         Int
   color     Int
   placedAt  DateTime @default(now())
   @@id([x, y])
}

datasource db {
   provider = "sqlite"
   url      = "file:./dev.db"
}